@phdthesis{Melo2011,
abstract = {Uma equa{\c{c}}{\~{a}}o diferencial {\'{e}} uma equa{\c{c}}{\~{a}}o que cont{\'{e}}m uma fun{\c{c}}{\~{a}}o desconhecida e algumas de suas derivadas. Essas equa{\c{c}}{\~{o}}es descrevem o comportamento de v{\'{a}}rios problemas f{\'{i}}sicos importantes aplicados na engenharia, como, por exemplo, a condu{\c{c}}{\~{a}}o de calor. A equa{\c{c}}{\~{a}}o diferencial que determina a distribui{\c{c}}{\~{a}}o de temperatura em um corpo {\'{e}} chamada de equa{\c{c}}{\~{a}}o de calor. A solu{\c{c}}{\~{a}}o dessa equa{\c{c}}{\~{a}}o pode ser obtida por m{\'{e}}todos anal{\'{i}}ticos e num{\'{e}}ricos. Os m{\'{e}}todos num{\'{e}}ricos consistem em reduzir um problema cont{\'{i}}nuo, com um n{\'{u}}mero infinito de vari{\'{a}}veis, em um problema discreto com um n{\'{u}}mero finito de vari{\'{a}}veis, podendo ser resolvido computacionalmente. Nesse trabalho, ser{\'{a}} apresentado a solu{\c{c}}{\~{a}}o de dois problemas de condu{\c{c}}{\~{a}}o de calor em regime transiente e unidimensional obtido pelo m{\'{e}}todo das diferen{\c{c}}as finitas expl{\'{i}}cito.},
author = {Melo, K. J.},
doi = {CDD 515.35},
file = {:home/carreira/Dropbox/Doutorado/Disciplinas/Modelagem num{\'{e}}rica de ondas s{\'{i}}smicas/Referencias/Kelly2011.pdf:pdf},
school = {UNIVERSIDADE FEDERAL RURAL DO SEMI-ARIDO CAMPOS ANGICOS},
title = {{Aplica{\c{c}}{\~{a}}o do m{\'{e}}todo das diferen{\c{c}}as finitas expl{\'{i}}cito na solu{\c{c}}{\~{a}}o da equa{\c{c}}{\~{a}}o do calor para o caso transiente e unidimensional}},
